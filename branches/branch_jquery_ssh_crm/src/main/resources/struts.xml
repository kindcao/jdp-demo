<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.custom.i18n.resources" value="messageResource" />
	<constant name="struts.devMode" value="false" />
	<package name="default" extends="tiles-default" namespace="/">
		<interceptors>
			<interceptor name="exceptionInterceptor" class="crm.interceptor.ExceptionInterceptor" />
			<interceptor name="checkLoginInterceptor" class="crm.interceptor.CheckLoginInterceptor" />
			<interceptor-stack name="myStack">
				<interceptor-ref name="defaultStack" />
				<interceptor-ref name="checkLoginInterceptor">
					<param name="excludeMethods">showLogin,login,getSysStatus</param>
				</interceptor-ref>
				<interceptor-ref name="exceptionInterceptor" />
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="myStack" />
		<global-results>
			<result name="login" type="tiles">syscompuser.login</result>
			<result name="error" type="tiles">error</result>
		</global-results>
		<global-exception-mappings>
			<exception-mapping result="error" exception="java.lang.Exception"></exception-mapping>
		</global-exception-mappings>
		<!-- comm -->
		<action name="welcome" class="commAction" method="welcome">
			<result name="welcome" type="tiles">welcome</result>
		</action>
		<action name="error" class="commAction" method="error" />
		<action name="getSysStatus" class="commAction" method="getSysStatus" />
		<action name="getStatusYN" class="commAction" method="getStatusYN" />
		<action name="getSysComp" class="commAction" method="getSysComp" />
		<action name="getCustIndu" class="commAction" method="getCustIndu" />
		<action name="getMarketEventType" class="commAction" method="getMarketEventType" />
		<action name="getSysCompUserByCompIds" class="commAction" method="getSysCompUserByCompIds" />
		<action name="getSysCompUserByUserIds" class="commAction" method="getSysCompUserByUserIds" />
		<!-- cust -->
		<action name="showCustList" class="custAction" method="showCustList">
			<result name="cust.broker.list" type="tiles">cust.broker.list</result>
			<result name="cust.bank.list" type="tiles">cust.bank.list</result>
			<result name="cust.invest.list" type="tiles">cust.invest.list</result>
			<result name="cust.media.list" type="tiles">cust.media.list</result>
			<result name="cust.info.list" type="tiles">cust.info.list</result>
			<result name="cust.listed.list" type="tiles">cust.listed.list</result>
			<result name="cust.others.list" type="tiles">cust.others.list</result>
		</action>
		<action name="showCustInfo" class="custAction" method="showCustInfo">
			<result name="cust.broker.info" type="tiles">cust.broker.info</result>
			<result name="cust.bank.info" type="tiles">cust.bank.info</result>
			<result name="cust.invest.info" type="tiles">cust.invest.info</result>
			<result name="cust.media.info" type="tiles">cust.media.info</result>
			<result name="cust.info.info" type="tiles">cust.info.info</result>
			<result name="cust.listed.info" type="tiles">cust.listed.info</result>
			<result name="cust.others.info" type="tiles">cust.others.info</result>
		</action>
		<action name="showCustInfoDtl" class="custAction" method="showCustInfoDtl">
			<result name="cust.info.dtl">/jsp/cust/custInfoDtl.jsp</result>
		</action>
		<action name="getCustList" class="custAction" method="getCustList" />
		<action name="getCustNameList" class="custAction" method="getCustNameList" />
		<action name="saveCustInfo" class="custAction" method="saveCustInfo" />
		<action name="deleteCust" class="custAction" method="deleteCust" />
		<!-- cust - contact  -->
		<action name="showContList" class="contAction" method="showContList">
			<result name="contact.list">/jsp/cust/contactList.jsp</result>
		</action>
		<action name="showContInfo" class="contAction" method="showContInfo">
			<result name="contact.info">/jsp/cust/contactInfo.jsp</result>
		</action>
		<action name="getContList" class="contAction" method="getContList" />
		<action name="getContByCustIds" class="contAction" method="getContByCustIds" />
		<action name="saveContInfo" class="contAction" method="saveContInfo" />
		<action name="deleteCont" class="contAction" method="deleteCont" />
		<!-- market event  -->
		<action name="showMktEvtList" class="mktEvtAction" method="showMktEvtList">
			<result name="mktevt.list" type="tiles">mktevt.list</result>
		</action>
		<action name="showMktEvtInfo" class="mktEvtAction" method="showMktEvtInfo">
			<result name="mktevt.info" type="tiles">mktevt.info</result>
		</action>
		<action name="getMktEvtList" class="mktEvtAction" method="getMktEvtList" />
		<action name="saveMktEvtInfo" class="mktEvtAction" method="saveMktEvtInfo" />
		<action name="deleteMktEvt" class="mktEvtAction" method="deleteMktEvt" />
		<!-- market event - cal -->
		<action name="showMktEvtCal" class="mktEvtAction" method="showMktEvtCal">
			<result name="mktevt.cal" type="tiles">mktevt.cal</result>
		</action>
		<action name="showMktEvtCalDtl" class="mktEvtAction" method="showMktEvtCalDtl">
			<result name="mktevt.cal.dtl">/jsp/mktevt/mktEvtCalDtl.jsp</result>
		</action>
		<action name="getMktEvtCal" class="mktEvtAction" method="getMktEvtCal" />
		<!-- market event - count -->
		<action name="showMktEvtCount" class="mktEvtAction" method="showMktEvtCount">
			<result name="mktevt.count" type="tiles">mktevt.count</result>
		</action>
		<action name="getMktEvtCountTab" class="mktEvtAction" method="getMktEvtCountTab" />
		<!-- syssetup company -->
		<action name="showSysCompList" class="sysCompAction" method="showSysCompList">
			<result name="syscomp.list" type="tiles">syscomp.list</result>
		</action>
		<action name="getSysCompList" class="sysCompAction" method="getSysCompList" />
		<action name="saveSysCompInfo" class="sysCompAction" method="saveSysCompInfo" />
		<action name="deleteSysCompInfo" class="sysCompAction" method="deleteSysCompInfo" />
		<!-- syssetup user -->
		<action name="showLogin" class="sysCompUserAction" method="showLogin" />
		<action name="login" class="sysCompUserAction" method="login" />
		<action name="logout" class="sysCompUserAction" method="logout" />
		<action name="showSysCompUserList" class="sysCompUserAction" method="showSysCompUserList">
			<result name="syscompuser.list" type="tiles">syscompuser.list</result>
		</action>
		<action name="getSysCompUserList" class="sysCompUserAction" method="getSysCompUserList" />
		<action name="saveSysCompUserInfo" class="sysCompUserAction" method="saveSysCompUserInfo" />
		<action name="deleteSysCompUser" class="sysCompUserAction" method="deleteSysCompUser" />
	</package>
</struts>